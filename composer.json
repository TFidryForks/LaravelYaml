{
    "name": "fidry/laravel-yaml",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "symfony/yaml": "^3.0",
        "symfony/config": "^3.0",
        "symfony/expression-language": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Fidry\\LaravelYaml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fidry\\LaravelYaml\\": "tests"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.dist', '.env');\""
        ]
    },
    "config": {
        "bin-dir": "bin/"
    }
}
